{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "description": "My wondrous Nuxt.js project",
  "author": "taka",
  "private": true,
  "scripts": {
    "lint-js": "eslint --ext .js,.vue .",
    "lint-style": "stylelint \"**/*.vue\" \"**/*.css\"",
    "lint-js:fix": "eslint --ext .js,.vue . --fix",
    "lint-style:fix": "stylelint \"**/*.vue\" \"**/*.css\" --fix",
    "format": "yarn prettier --write \"**/*.{js, vue}\"",
    "lint:check": "yarn lint-js & yarn lint-style",
    "lint:fix": "yarn lint-js:fix && yarn lint-style:fix && yarn format",
    "test": "jest",
    "test:cov": "jest --coverage",
    "cy": "cypress run",
    "cy:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 cy",
    "e2e:open": "start-server-and-test dev http://localhost:3000 cy:open",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt build",
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt start",
    "generate": "export NODE_OPTIONS=--openssl-legacy-provider && nuxt generate",
    "dev:sb": "export NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build:sb": "export NODE_OPTIONS=--openssl-legacy-provider && build-storybook --no-dll"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "2.4.0",
    "dayjs": "1.11.10",
    "express": "^4.18.2",
    "firebase": "10.4.0",
    "github-markdown-css": "5.2.0",
    "highlight.js": "^11.3.1",
    "marked": "4.3.0",
    "normalize.css": "8.0.1",
    "nuxt": "2.17.3",
    "vue-lazyload": "1.3.5",
    "vue-scrollto": "2.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.22.20",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/dotenv": "1.4.1",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/cli": "6.5.16",
    "@storybook/vue": "6.5.16",
    "@vue/test-utils": "1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.3.0",
    "core-js": "3.32.2",
    "css-loader": "5.2.7",
    "cypress": "^13.2.0",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "9.17.0",
    "firebase-tools": "12.5.4",
    "jest": "^27.5.1",
    "mockdate": "^3.0.5",
    "postcss-html": "^1.3.0",
    "prettier": "3.0.3",
    "react-is": "18.2.0",
    "start-server-and-test": "^2.0.0",
    "storybook-vue-router": "1.0.7",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.3.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "^25.0.0",
    "vue-eslint-parser": "9.3.2",
    "vue-jest": "3.0.7",
    "vue-loader": "15.10.2"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
