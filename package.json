{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "description": "My wondrous Nuxt.js project",
  "author": "taka",
  "private": true,
  "scripts": {
    "lint-js": "eslint --ext .js,.vue .",
    "lint-style": "stylelint \"**/*.vue\" \"**/*.css\"",
    "lint-js:fix": "eslint --ext .js,.vue . --fix",
    "lint-style:fix": "stylelint \"**/*.vue\" \"**/*.css\" --fix",
    "format": "yarn prettier --write \"**/*.{js, vue}\"",
    "lint:check": "yarn lint-js & yarn lint-style",
    "lint:fix": "yarn lint-js:fix && yarn lint-style:fix && yarn format",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "test:cov": "jest --coverage",
    "cy": "cypress run",
    "cy:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 cy",
    "e2e:open": "start-server-and-test dev http://localhost:3000 cy:open",
    "dev:sb": "start-storybook -p 6006",
    "build:sb": "build-storybook --no-dll"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "2.4.0",
    "dayjs": "1.11.7",
    "firebase": "9.6.11",
    "github-markdown-css": "5.1.0",
    "highlight.js": "^11.3.1",
    "marked": "4.0.16",
    "normalize.css": "8.0.1",
    "nuxt": "2.15.8",
    "vue-lazyload": "1.3.4",
    "vue-scrollto": "2.20.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-private-methods": "7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/dotenv": "1.4.1",
    "@storybook/addon-actions": "6.5.15",
    "@storybook/addon-essentials": "6.5.15",
    "@storybook/addon-links": "6.5.15",
    "@storybook/cli": "6.5.15",
    "@storybook/vue": "6.5.15",
    "@vue/test-utils": "1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.5",
    "babel-preset-vue": "2.0.2",
    "core-js": "3.21.1",
    "css-loader": "5.2.7",
    "cypress": "9.5.4",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "8.5.0",
    "firebase-tools": "10.4.2",
    "jest": "^27.5.1",
    "postcss-html": "^1.3.0",
    "prettier": "2.6.2",
    "react-is": "18.2.0",
    "start-server-and-test": "^1.14.0",
    "storybook-vue-router": "1.0.7",
    "stylelint": "14.6.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "vue-eslint-parser": "8.3.0",
    "vue-jest": "3.0.7",
    "vue-loader": "15.9.8"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
